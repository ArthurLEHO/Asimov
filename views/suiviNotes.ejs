<%- include('header') %>
    <!DOCTYPE html>
    <html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>ASIMOV</title>
    </head>

    <body>
        <table class="table text-center">
            <thead class="thead-dark">
                <tr>
                    <th class="bg-dark text-white" scope="col">Élève</th>
                    <th class="bg-dark text-white" scope="col">Résultat</th>
                    <th class="bg-dark text-white" scope="col">Matière</th>
                    <th class="bg-dark text-white" scope="col">Date</th>
                    <th class="bg-dark text-white" scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                <% dataListeNotes.forEach((note, index)=>{%>
                    <tr id=<%=note.nt_id%>>
                        <td>
                            <%=note.el_nom%>
                                <%=note.el_prenom%>
                        </td>
                        <td>
                            <%=note.nt_resultat%>
                        </td>
                        <td>
                            <%=note.mt_nom%>
                        </td>
                        <td>
                            <%=note.nt_date%>
                        </td>
                        <td>
                            <div class="btn-group" role="group">
                                <button type="button" class="btn btn-secondary">Modifier</button>
                                <button type="button" class="btn btn-danger">Supprimer</button>
                            </div>
                        </td>
                    </tr>
                    <%})%>
            </tbody>
        </table>
    </body>

    </html>
    <%- include('footer') %>